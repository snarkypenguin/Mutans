'Parameters

(define param:map-maker (/ (* 150 kg) (* 37 years)))

(define map-maker-parameters ;; Parameters for Botanicus exemplarii
  (list
  	(list 'name "map-maker")
	(list 'format 'ps)
	(list 'filename "map-")
	(list 'filetype "0.ps")
  )
)
(set! global-parameter-alist (cons (cons "map-maker" map-maker-parameters) global-parameter-alist))







