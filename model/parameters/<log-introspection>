; -*- mode: scheme; -*-
'Parameters
(define <log-introspection>-parameters
  (list
	 (list 'report-time-register '($ (make-table))) ;; This is done on a by/agent basis for obvious reasons
	 ;;(list 'type 'logger)
	 (list 'dt (* 1 day))
    ;;(list 'file '<uninitialised>)
    (list 'filename #f)
    (list 'filetype #f)
	 (list 'filename-timescale #f)
    (list 'format 'text)
    (list 'variables '())
    (list 'variables-may-be-set '#t)
    (list 'missing-val 'NoData)
    (list 'show-field-name #f)
    (list 'preamble-state '())
    (list 'timestep-epsilon 1e-6)
	 (list 'default-font 'Helvetica)
	 (list 'default-size 10) ;; point
	 (list 'default-color ps-black)
  )
)
(set! global-parameter-alist (cons (cons <log-introspection> <log-introspection>-parameters) global-parameter-alist))
