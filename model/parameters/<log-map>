'Parameters

(define <log-map>-parameters
  (list
  	 (list 'type 'log-map)
	 (list 'format 'ps)
	 (list 'file-type "ps")
    (list 'model->local '($ (cdr (assoc 'model->ps *default-projections*)))) ;; Takes model space to ps page
	 (list 'local->model '($ (cdr (assoc 'ps->model *default-projections*)))) ;; ... and the other way 'round.
    ;;(list 'ps '<uninitialised>)
    ;;(list 'png '<uninitialised>)
  )
)
(set! global-parameter-alist (cons (cons <log-map> <log-map>-parameters) global-parameter-alist))
