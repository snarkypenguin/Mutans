; -*- mode: scheme; -*-
'Parameters
(define <snapshot>-parameters
  (list
    (list 'type 'snapshot)
    (list 'lastfile #f)
    (list 'currentfile #f)
	 (list 'filename-timescale #f) ;; defaults to a single file
	 (list 'use-separate-files #f)
	 (list 'pagecount 1)
	 ;(list 'filename-timescale days)
	 (list 'no-model-body #t)
  )
)
(set! global-parameter-alist (cons (cons <snapshot> <snapshot>-parameters) global-parameter-alist))
