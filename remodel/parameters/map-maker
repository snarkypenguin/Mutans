; -*- mode: scheme; -*-
'Parameters


(define map-maker-parameters
  (list
	(list 'format 'ps)
	(list 'filename "map")
	;; the time is inserted between the "filename" and the "filetype"
	;; if the timestep is specified
	
	(list 'filetype "ps")
  )
)
(set! global-parameter-alist (cons (cons "map-maker" map-maker-parameters) global-parameter-alist))







