'Parameters
(define <animal>-parameters
  (list
    (list 'current-interest '())
    (list 'movementspeed '<uninitialized>)
    (list 'searchspeed '<uninitialized>)
    (list 'foragespeed '<uninitialized>)
    (list 'wanderspeed '<uninitialized>)
    (list 'objective '<uninitialized>)
    (list 'near-food-attraction '<uninitialized>)
  )
)
(set! global-parameter-alist (cons (cons <animal> <animal>-parameters) global-parameter-alist))
